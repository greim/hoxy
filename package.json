{
  "name": "hoxy",
  "version": "3.3.1",
  "author": "Greg Reimer <gregreimer@gmail.com>",
  "description": "Web-hacking proxy API for node",
  "repository": {
    "type": "git",
    "url": "git@github.com:greim/hoxy.git"
  },
  "main": "./lib/main",
  "scripts": {
    "prepublishOnly": "npm run-script compile",
    "test": "mocha --require babel-core/register --timeout 3000 --exit",
    "compile": "babel src --out-dir lib --optional runtime",
    "lint": "eslint ./src",
    "docs": "babel-node ./dev-bin/build.js > ./index.html"
  },
  "keywords": [
    "develop",
    "development",
    "test",
    "testing",
    "debug",
    "debugging",
    "proxy",
    "charles",
    "fiddler",
    "web"
  ],
  "dependencies": {
    "await": "^0.2.5",
    "babel-runtime": "^6.26.0",
    "bluebird": "^2.9.34",
    "cheerio": "^0.22.0",
    "co": "^4.5.4",
    "colors": "^1.1.2",
    "lodash": "^4.17.5",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.1",
    "node-static": "^0.7.6",
    "nodemon": "^1.0.17",
    "pem": "^1.7.2",
    "route-pattern": "^0.0.6",
    "stream-throttle": "^0.1.3",
    "ugly-adapter": "^1.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-core": "^6.26.0",
    "eslint": "^5.1.0",
    "mocha": "^5.2.0"
  }
}
